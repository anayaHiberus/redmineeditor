<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>
<VBox xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.hiberus.anaya.redmineeditor.dialogs.IssueDetailsController">
    <Label fx:id="description" maxWidth="1.7976931348623157E308" text="Issue description" VBox.vgrow="ALWAYS">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
    </Label>
    <WebView fx:id="webview" VBox.vgrow="ALWAYS"/>
    <ButtonBar>
        <buttons>
            <Button onAction="#openInRedmine" text="Open in _Redmine"/>
            <Button cancelButton="true" defaultButton="true" layoutX="486.0" layoutY="17.0" onAction="#close"
                    text="_Close"/>
        </buttons>
        <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </VBox.margin>
    </ButtonBar>
</VBox>
