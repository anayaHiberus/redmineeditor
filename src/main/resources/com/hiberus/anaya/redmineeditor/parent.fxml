<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<StackPane prefHeight="640.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.2"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hiberus.anaya.redmineeditor.controllers.ParentCtrl">
    <children>
        <VBox fx:id="parent" prefHeight="200.0" prefWidth="100.0">
            <children>
                <fx:include fx:id="settings" source="settings.fxml" VBox.vgrow="NEVER"/>
                <Separator prefWidth="200.0">
                    <padding>
                        <Insets bottom="10.0" top="10.0"/>
                    </padding>
                </Separator>
                <fx:include fx:id="calendar" source="calendar.fxml" VBox.vgrow="NEVER"/>
                <Separator prefWidth="200.0">
                    <padding>
                        <Insets bottom="10.0" top="10.0"/>
                    </padding>
                </Separator>
                <fx:include fx:id="summary" source="summary.fxml" VBox.vgrow="NEVER"/>
                <Separator prefWidth="200.0">
                    <padding>
                        <Insets bottom="10.0" top="10.0"/>
                    </padding>
                </Separator>
                <fx:include fx:id="entries" source="entries.fxml" VBox.vgrow="ALWAYS"/>
                <Separator prefWidth="200.0">
                    <padding>
                        <Insets bottom="10.0" top="10.0"/>
                    </padding>
                </Separator>
                <fx:include fx:id="actions" source="actions.fxml"/>
            </children>
        </VBox>
        <ProgressIndicator fx:id="progress"/>

    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</StackPane>
