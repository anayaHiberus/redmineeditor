<?xml version="1.0" encoding="UTF-8"?>

<!--suppress ALL -->


<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<StackPane xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="com.hiberus.anaya.redmineeditor.components.ParentComponent">
    <children>
        <StackPane fx:id="parent" prefHeight="720.0" prefWidth="1280.0">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <VBox>
                <fx:include fx:id="calendar" source="calendar.fxml" VBox.vgrow="NEVER"/>
                <Separator prefWidth="200.0">
                    <padding>
                        <Insets bottom="10.0" top="10.0"/>
                    </padding>
                </Separator>
                <fx:include fx:id="summary" source="summary.fxml" VBox.vgrow="NEVER"/>
                <Separator prefWidth="200.0">
                    <padding>
                        <Insets bottom="10.0" top="10.0"/>
                    </padding>
                </Separator>
                <fx:include fx:id="entries" source="entries.fxml" VBox.vgrow="ALWAYS"/>
                <Separator prefWidth="200.0">
                    <padding>
                        <Insets bottom="10.0" top="10.0"/>
                    </padding>
                </Separator>
                <fx:include fx:id="insert" source="insert.fxml" VBox.vgrow="NEVER"/>
                <Separator prefWidth="200.0">
                    <padding>
                        <Insets bottom="10.0" top="10.0"/>
                    </padding>
                </Separator>
                <fx:include fx:id="actions" source="actions.fxml" VBox.vgrow="NEVER"/>
            </VBox>
            <Button mnemonicParsing="false" onAction="#showReadme" text="?" StackPane.alignment="TOP_RIGHT">
                <tooltip>
                    <Tooltip text="Show help"/>
                </tooltip>
            </Button>
            <Button mnemonicParsing="false" onAction="#showSettings" text="settings" StackPane.alignment="TOP_LEFT">
                <tooltip>
                    <Tooltip text="Show settings"/>
                </tooltip>
            </Button>

        </StackPane>
        <ProgressIndicator fx:id="progress"/>
    </children>
</StackPane>
